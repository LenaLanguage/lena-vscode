{
  "name": "lena",
  "displayName": "Lena programming language",
  "description": "Basic grammar highlighting for the Lena programming language.",
  "publisher": "robotsvision",
  "icon": "images/icon.png",
  "version": "0.0.1",
  "repository": "https://github.com/robotsvision/lena-lang-vscode",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "iconThemes": [
      {
        "id": "lenaIcons",
        "label": "Lena Icons",
        "path": "language-icon_theme.json"
      }
    ],
    "languages": [{
      "id": "lena",
      "aliases": ["Lena", "lena"],
      "extensions": [".le"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "lena",
      "scopeName": "source.lena",
      "path": "./syntaxes/lena.tmLanguage.json"
    }]
  }
}
